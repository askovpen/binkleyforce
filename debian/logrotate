	daily
	rotate 4
	missingok
	notifempty
	compress
	notifempty
	create 644 uucp news
}

/var/log/bforce/outman.log {
	weekly
	rotate 4
	missingok
	notifempty
	compress
	notifempty
	nocreate
}

